{
    "rules": [
        {
            "name": "Screenshots",
            "match": {
                "contains": ["screenshot"],
                "extensions": [".png", ".jpg", ".jpeg"]
            },
            "destination": "Screenshots"
        },
        {
            "name": "Software2 Homework",
            "match": {
                "contains": ["software2", "hw"],
                "extensions": [".pdf", ".docx", ".doc"]
            },
            "destination": "School/Software2/Homework"
        },
        {
            "name": "Java Code",
            "match": {
                "extensions": [".java"]
            },
            "destination": "Code"
        },
        {
            "name": "PDF Documents",
            "match": {
                "extensions": [".pdf"]
            },
            "destination": "Docs/PDFs"
        },
        {
            "name": "Images",
            "match": {
                "extensions": [".png", ".jpg", ".jpeg", ".gif", ".bmp", ".svg"]
            },
            "destination": "Images"
        }
    ]
}
